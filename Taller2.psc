Proceso ejercicio_1
	Definir a , b Como Entero;
	a <- 1;
	b <- 0;
	Mientras a >= b Hacer
		Escribir "Ingrese el primer numero";
		Leer a;
		Escribir "Ingrese el segundo numero";
		Leer b;
	FinMientras
	
	
	i <- b - 1;
	
	Mientras  i  > a Hacer
		Escribir i;
		i <- i - 1 ;
	FinMientras
FinProceso
